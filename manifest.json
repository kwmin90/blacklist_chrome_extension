{
    "name": "Manga Title Hider",
    "version" : "1.0",
    "description": "Hides html elements with corresponding manga title",
    "permissions": ["tabs", "storage", "https://manatoki95.net/*", "https://newtoki95.com/*"],
    "background": {
        "scripts":["background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "options_page": "options.html",
    "manifest_version": 2
}